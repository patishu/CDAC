<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	
	<bean name="/register.html" class="springHibernate.insertCustomer"></bean>
	<bean name="/login.html" class="springHibernate.loginvalidate"></bean>
	<bean name="/agentregister.html" class="springHibernate.loginAgent"></bean>
	<bean name="/agentLog.html" class="springHibernate.agentValidate"></bean>
	<bean name="/insertPack.html" class="springHibernate.insertPackage"></bean>
	
	<bean id="viewResolver" name="viewResolver"
		class="org.springframework.web.servlet.view.InternalResourceViewResolver">
		<property name="prefix" value="/"></property>
		<property name="suffix" value=".jsp"></property>
	</bean>
</beans>
